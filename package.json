{
  "name": "@adipundir/aptos-x402",
  "version": "0.1.2",
  "description": "Official x402 Payment Protocol SDK for Aptos blockchain",
  "author": "Adi Pundir",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adipundir/aptos-x402"
  },
  "homepage": "https://aptos-x402.vercel.app",
  "keywords": [
    "x402",
    "aptos",
    "payment",
    "micropayment",
    "http-402",
    "blockchain",
    "cryptocurrency"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    }
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run build:sdk && npm run build:demo",
    "build:sdk": "tsc -p tsconfig.sdk.json",
    "build:demo": "next build --turbopack",
    "start": "next start",
    "demo": "tsx scripts/demo-agent.ts",
    "prepublishOnly": "npm run build:sdk",
    "test": "echo \"Tests coming soon\" && exit 0"
  },
  "peerDependencies": {
    "@aptos-labs/ts-sdk": ">=1.26.0",
    "next": ">=15.0.0"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": ">=1.26.0",
    "@langchain/core": "^0.3.0",
    "@langchain/google-genai": "^0.1.0",
    "@polkadot/keyring": "^13.5.6",
    "@polkadot/util": "^13.5.6",
    "@vercel/analytics": "^1.5.0",
    "express": "^5.1.0",
    "langchain": "^0.3.0",
    "next": ">=15.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "x402-vara": "^0.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^4",
    "tsx": "^4.7.1",
    "typescript": "^5"
  }
}
